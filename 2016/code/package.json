{
  "name": "lifia-semantic-augmentation",
  "version": "0.1.0",
  "scripts": {
    "build": "babel es6/src -d js/lib",
    "uglify": "uglifyjs js/lib/* -o lsa.min.js",
    "test": "node lsa.min.js",
    "myug": "rm final.js; for f in `ls -1 es6/src`; do cat es6/src/$f >> final.js; done",
    "mytest": "node finalfinal.js",
    "buildf": "babel final.js >finalfinal.js"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.6.0"
  },
  "dependencies": {
    "jsdom": "^8.4.0",
    "sync-request": "^3.0.0",
    "xmldom": "^0.1.22"
  }
}
